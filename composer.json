{
  "name": "upstatement/wordpress-starter-kit",
  "authors": [
    {
      "name": "Upstatement",
      "email": "tech@upstatement.com"
    }
  ],
  "description": "",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com"
    }
  ],
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": [
        "wpackagist-plugin/*",
        "type:wordpress-plugin"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0"
    },
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "vendor-dir": "themes/wsk/vendor"
  },
  "autoload": {
    "psr-4": {
      "WordPressStarterKit\\": "themes/wsk/includes"
    }
  },
  "require": {
    "wpackagist-plugin/wp-environment-indicator": "^1.0",
    "vlucas/phpdotenv": "^5.4",
    "timber/timber": "^1.22",
    "wpengine/advanced-custom-fields-pro": "^6.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6",
    "friendsofphp/php-cs-fixer": "^3.3",
    "symfony/var-dumper": "^4.4",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
    "wp-coding-standards/wpcs": "^2.3",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "szepeviktor/phpstan-wordpress": "^1.0",
    "php-stubs/acf-pro-stubs": "^5.10"
  }
}
